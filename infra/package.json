{
  "name": "infra",
  "version": "0.1.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build:app": "cd ../apps/api && pnpm build",
    "build:worker": "cd ../apps/worker && pnpm build",
    "cdk:deploy": "pnpm build && cdk deploy",
